{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build",
        "^pipeline:build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "lint:fix": {
      "dependsOn": [
        "^lint:fix"
      ]
    },
    "start:dev": {
      "dependsOn": [
        "^pipeline:build"
      ],
      "cache": false,
      "persistent": true
    },
    "prisma:generate": {
      "with": [
        "backend#prisma:generate"
      ],
      "cache": false,
      "persistent": true
    },
    "pipeline:build": {
      "cache": true,
      "persistent": false
    }
  }
}