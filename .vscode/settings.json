{
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "prisma.fileWatcher": true,
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "server": "localhost",
      "port": 6432,
      "driver": "PostgreSQL",
      "name": "auto_strategy",
      "group": "localhost",
      "database": "auto_strategy",
      "username": "admin",
      "password": "password"
    }
  ],
  "prisma.scriptRunner": "pnpm",
  "prisma.schemaPath": "/apps/backend/src/core/db/schemes/schema.prisma",
  "prisma.trace.server": "messages",
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": "\\.(prisma)$",
        "cmd": "turbo run prisma:generate",
        "background": true
      }
    ]
  }
}