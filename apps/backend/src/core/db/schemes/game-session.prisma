model GameSession {
  id          String            @id @default(uuid())
  map_id      String?           @map("mapId")
  status      GameSessionStatus
  created_at  DateTime          @default(now()) @map("createdAt")
  finished_at DateTime?         @map("finishedAt")

  @@map("game_sessions")
}

enum GameSessionStatus {
  WAITING
  IN_PROGRESS
  FINISHED
}
