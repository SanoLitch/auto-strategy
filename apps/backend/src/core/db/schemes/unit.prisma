model Unit {
  id          String   @id @default(uuid())
  player_id   String   @map("playerId")
  building_id String   @map("buildingId")
  type        UnitType
  position    Json
  health      Int
  player      Player   @relation(fields: [player_id], references: [id])
  building    Building @relation(fields: [building_id], references: [id])

  @@map("units")
}

enum UnitType {
  DIGGER
  MINER
  WARRIOR
  SCOUT
}
