{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./nest.json",
  "compilerOptions": {
    "outDir": "${configDir}/dist",
    "rootDir": "${configDir}/src"
  },
  "include": [
    "${configDir}/src/**/*"
  ],
  "exclude": [
    "${configDir}/node_modules",
    "${configDir}/dist"
  ]
}